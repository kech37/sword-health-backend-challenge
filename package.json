{
  "name": "sword-health-backend-challenge",
  "version": "1.0.0",
  "description": "Sword Health Backend Challenge",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "build:clean": "del-cli dist && npm run build",
    "dev": "npm run build:clean && cd dist/ && clear && dotenv -e ../.env node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db-migration:generate": "npx dotenv -e .env -- npx typeorm-ts-node-commonjs migration:generate ./src/db/migrations/%npm_config_name% -d ./src/configs/db-config.ts -p",
    "db-migration:run": "npx dotenv -e .env -- npx typeorm-ts-node-commonjs -d ./src/configs/db-config.ts migration:run",
    "db-migration:revert": "npx dotenv -e .env -- npx typeorm-ts-node-commonjs -d ./src/configs/db-config.ts migration:revert"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/node": "^20.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "del-cli": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "mysql2": "^3.6.1",
    "npm": "^10.1.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  }
}
