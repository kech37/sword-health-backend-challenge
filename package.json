{
  "name": "sword-health-backend-challenge",
  "version": "1.0.0",
  "description": "Sword Health Backend Challenge",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "build:clean": "del-cli dist && npm run build",
    "dev": "npm run build:clean && cd dist/ && clear && dotenv -e ../.env node index.js | npx pino-pretty",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db-migration:generate": "npx dotenv -e .env -- npx typeorm-ts-node-commonjs migration:generate ./src/db/migrations/%npm_config_name% -d ./src/configs/migrations-database-config.ts -p",
    "db-migration:run": "npx dotenv -e .env -- npx typeorm-ts-node-commonjs -d ./src/configs/migrations-database-config.ts migration:run",
    "db-migration:revert": "npx dotenv -e .env -- npx typeorm-ts-node-commonjs -d ./src/configs/migrations-database-config.ts migration:revert",
    "db-migration:create": "npx dotenv -e .env -- npx typeorm-ts-node-commonjs migration:create ./src/db/migrations/%npm_config_name%",
    "lint": "npx eslint src",
    "checkcode": "npm run lint",
    "lint:fix": "npx eslint --fix src",
    "setup:dev": "npm ci && npx dotenv -e .env -- npx typeorm-ts-node-commonjs -d ./src/configs/setup-database-config.ts migration:run && npm run build",
    "start": "cd dist/ && clear && dotenv -e ../.env node index.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "^0.10.2",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.7.1",
    "@types/uuid": "^9.0.4",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-no-template-curly-in-string-fix": "^1.0.4",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "del-cli": "^5.1.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^6.0.0",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "express-bearer-token": "^2.4.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.1",
    "npm": "^10.1.0",
    "pino": "^8.15.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "typeorm-encrypted": "^0.8.0",
    "uuid": "^9.0.1"
  }
}
